{
  "builds": [
    {
      "src": "ai-chat-app-new/package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/ai-chat-app-new/$1"
    }
  ],
  "functions": {
    "ai-chat-app-new/src/app/api/**/*.ts": {
      "maxDuration": 60
    }
  }
}
