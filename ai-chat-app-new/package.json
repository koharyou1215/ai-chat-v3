{
  "name": "ai-chat-app-new",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.x",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "_prebuild_disabled": "node scripts/generate-characters-manifest.js && node scripts/generate-personas-manifest.js",
    "prebuild": "tsx scripts/generate-manifests.ts",
    "build": "next build",
    "_postbuild_disabled": "tsx scripts/validate-manifests.ts",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "prepare": "husky install",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "ANALYZE=true npm run build && echo 'Bundle analyzer available at http://localhost:8888'",
    "analyze:prod": "cross-env NODE_ENV=production ANALYZE=true npm run build && echo 'Production bundle analysis saved to bundle-analysis/'",
    "analyze:compare": "node scripts/bundle-compare.js",
    "perf:test": "node scripts/performance-test.js",
    "perf:build": "npm run build && npm run perf:test",
    "perf:lighthouse": "node scripts/lighthouse-test.js",
    "perf:monitor": "node scripts/bundle-monitor.js",
    "budget:check": "node scripts/budget-check.js",
    "budget:report": "npm run analyze:prod && npm run budget:check",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next out node_modules/.cache bundle-analysis",
    "clean:build": "npm run clean && npm run build",
    "preanalyze:prod": "if not exist bundle-analysis mkdir bundle-analysis",
    "postanalyze:prod": "node scripts/post-analysis.js",
    "golden-master:generate": "tsx tests/golden-master/generate-golden-master.ts",
    "golden-master:test": "jest tests/golden-master/compare-prompts.test.ts",
    "golden-master:full": "npm run golden-master:generate && npm run golden-master:test",
    "validate:prompt-v2": "tsx scripts/validate-prompt-v2.ts",
    "validate:phase1": "tsx scripts/validate-phase1-simple.ts"
  },
  "dependencies": {
    "@composio/mcp": "^1.0.9",
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@vercel/blob": "^1.1.1",
    "axios": "^1.12.0",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.11.11",
    "lucide-react": "^0.469.0",
    "lz-string": "^1.5.0",
    "next": "^15.5.4",
    "next-themes": "^0.4.6",
    "openai": "^4.84.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.6.0",
    "react-hotkeys-hook": "^4.6.1",
    "react-markdown": "^10.1.0",
    "react-player": "^2.16.0",
    "react-use": "^17.5.1",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.3.7",
    "uuid": "^11.0.3",
    "zod": "^3.23.8",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/lz-string": "^1.5.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.35.0",
    "eslint-config-next": "^15.1.3",
    "happy-dom": "^20.0.4",
    "husky": "^9.1.7",
    "postcss": "^8",
    "rimraf": "^5.0.10",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
